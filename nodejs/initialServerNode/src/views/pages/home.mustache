{{>partials/header}}

    {{#welcome}}
    <p> Opá, tudo bem? {{ user.username }} </p>
    {{/welcome}}

    <hr/>

    <h2>Produtos</h2>


    <ul>
    {{#products}}
        <li> {{ title }} - R${{ price }} </li>
    {{/products}}
    </ul>

    <hr/>

    <h2>Frases</h2>


    <ul>
    {{#list}}
        <li> {{ . }} </li>
    {{/list}}
    {{^list}}
        <p>Esse campo com o ^ é como se fosse o else, caso de vazio por exemplo</p>
    {{/list}}
    </ul>

{{>partials/footer}}